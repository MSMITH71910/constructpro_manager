{
  "tasks": {
    "start": "deno run --allow-net --allow-read --allow-write --allow-env --allow-sys server.ts",
    "dev": "deno run --allow-net --allow-read --allow-write --allow-env --allow-sys --watch server.ts"
  },
  "nodeModulesDir": "auto",
  "imports": {
    "express": "npm:express@4.18.2",
    "cors": "npm:cors@2.8.5"
  },
  "compilerOptions": {
    "allowJs": true,
    "lib": ["deno.window"],
    "strict": true
  },
  "exclude": [
    "node_modules",
    "tests",
    "assets"
  ]
}